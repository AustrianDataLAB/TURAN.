{
  "clients": [
    {
      "attributes": {
        "saml.server.signature": "true"
      },
      "clientId": "https://guacamole/guacamole",
      "defaultClientScopes": [],
      "name": "Guacamole",
      "protocol": "saml",
      "protocolMappers": [
        {
          "config": {
            "attribute.name": "groups",
            "full.path": "false",
            "single": "true"
          },
          "consentRequired": false,
          "name": "Group list",
          "protocol": "saml",
          "protocolMapper": "saml-group-membership-mapper"
        },
        {
          "config": {
            "mapper.nameid.format": "urn:oasis:names:tc:SAML:2.0:nameid-format:persistent",
            "user.attribute": "username"
          },
          "consentRequired": false,
          "name": "User Attribute Mapper for NameID",
          "protocol": "saml",
          "protocolMapper": "saml-user-attribute-nameid-mapper"
        }
      ],
      "redirectUris": [
        "https://guacamole/guacamole/*"
      ]
    }
  ],
  "enabled": true,
  "realm": "test"
}
